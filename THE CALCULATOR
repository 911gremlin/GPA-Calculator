# CP Class
#Lists 
classes= []
grades = []

#Collects the data of Class names and Grades in Letter Form
def collect():
    i = 0
    while (i <= 5):
        className = print("Enter Class Name: ")
        classes.append(className)
        i = i +1


    print(classes)
    y = 0
    while (y <=5):
        grade = print("Enter Your Grade For Each Class Listed in Order (Letter Form): ")
        grade = grade.upper()
        grades.append(grade)
        y = y + 1
    calculate()

def calculate():
    total= 0
    for element in grades:
        if element == "A":
            total = total + 4.0
        elif element == "B":
            total = total + 3.0
        elif element == "C":
            total = total + 2.0
        elif element == "D":
            total = total + 1.0
        else:
            total = total + 0.0
      
    gpa = total / classes
    print(gpa)


collect()
# Honors Class
#Lists 
classes= []
grades = []

#Collects the data of Class names and Grades in Letter Form
def collect():
    i = 0
    while (i <= 5):
        className = print("Enter Class Name: ")
        classes.append(className)
        i = i +1


    print(classes)
    y = 0
    while (y <=5):
        grade = input("Enter Your Grade For Each Class Listed in Order (Letter Form): ")
        grade = grade.upper()
        grades.append(grade)
        y = y + 1
    calculate()

def calculate():
    total= 0
    for element in grades:
        if element == "A":
            total = total + 4.5
        elif element == "B":
            total = total + 3.5
        elif element == "C":
            total = total + 2.5
        elif element == "D":
            total = total + 1.5
        else:
            total = total + 0.0
      
    gpa = total / classes
    print(gpa)


collect()
# AP Class
#Lists 
classes= []
grades = []

#Collects the data of Class names and Grades in Letter Form
def collect():
    i = 0
    while (i <= 5):
        className = print("Enter Class Name: ")
        classes.append(className)
        i = i +1


    print(classes)
    y = 0
    while (y <=5):
        grade = input("Enter Your Grade For Each Class Listed in Order (Letter Form): ")
        grade = grade.upper()
        grades.append(grade)
        y = y + 1
    calculate()

def calculate():
    total= 0
    for element in grades:
        if element == "A":
            total = total + 5.0
        elif element == "B":
            total = total + 4.0
        elif element == "C":
            total = total + 3.0
        elif element == "D":
            total = total + 2.0
        else:
            total = total + 0.0
      
    gpa = total / classes
    print(gpa)


collect()
